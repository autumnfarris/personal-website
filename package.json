{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
	"homepage": "https://autumnfarris.github.io/personal-website",
  "scripts": {
    "start": "next start",
    "lint": "next lint",
    "predeploy": "npm run build",
		"dev": "next dev",
		"clean": "rimraf .next out dist",
